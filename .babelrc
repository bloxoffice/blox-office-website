{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "assets": "./src/assets",
          "containers": "./src/containers",
          "components": "./src/components",
          "constants": "./src/constants",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
